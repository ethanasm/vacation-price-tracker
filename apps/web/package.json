{
  "name": "vacation-price-tracker-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:http": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint .",
    "format": "biome format .",
    "typecheck": "tsc --noEmit",
    "test": "NODE_NO_WARNINGS=1 jest",
    "test:watch": "NODE_NO_WARNINGS=1 jest --watch",
    "test:coverage": "NODE_NO_WARNINGS=1 jest --coverage",
    "generate:api-types": "cd ../api && uv run python scripts/export_openapi.py ../web/src/lib/api/openapi.json && cd ../web && npx openapi-typescript src/lib/api/openapi.json -o src/lib/api/types.ts",
    "generate:api-types:check": "cd ../api && uv run python scripts/export_openapi.py /tmp/openapi-check.json && cd ../web && diff -q /tmp/openapi-check.json src/lib/api/openapi.json"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.0",
    "react-google-button": "^0.8.0",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "20.14.10",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.4.23",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.6",
